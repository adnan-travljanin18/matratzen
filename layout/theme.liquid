<script>
(function() {
    var originalOpen = XMLHttpRequest.prototype.open;
    var originalSend = XMLHttpRequest.prototype.send;

    XMLHttpRequest.prototype.open = function(method, url) {
        this.addEventListener('load', function() {
            if (url.includes('/cart/add')) {
                try {
                    var response = JSON.parse(this.responseText);
                    var items = response.items.map(function(item) {
                        return {
                            item_name: item.title,
                            item_id: item.id,
                            price: parseFloat(item.price),
                            quantity: parseInt(item.quantity, 10)
                        };
                    });

                    // Push the cart data to dataLayer
                    window.dataLayer = window.dataLayer || [];
                    window.dataLayer.push({
                        event: "form_submit",
                        ecommerce: {
                            items: items
                        },
                        cart_total: response.items.reduce(function(total, item) {
                            return total + parseFloat(item.price) * item.quantity;
                        }, 0),
                        cart_item_count: response.items.length
                    });

                    console.log('Cart Add Event:', {
                        items: items,
                        cart_total: response.items.reduce(function(total, item) {
                            return total + parseFloat(item.price) * item.quantity;
                        }, 0),
                        cart_item_count: response.items.length
                    });
                } catch (e) {
                    console.error('Error parsing cart add response:', e);
                }
            }
        });

        originalOpen.apply(this, arguments);
    };

    XMLHttpRequest.prototype.send = function() {
        originalSend.apply(this, arguments);
    };
})();
</script>


